    "version": "0.2.0",
    "configurations": [
        {
            "name": "websocket_client",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/websocket_examples/websocket_client/client.py",
            "console": "integratedTerminal"
        },
        {
            "name": "websocket_server",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
                "--host",
                "0.0.0.0",
                "--port",
                "8000"
            ],
            "cwd": "${workspaceFolder}/websocket_examples/websocket_server",
            "jinja": true
        }
    ]
}